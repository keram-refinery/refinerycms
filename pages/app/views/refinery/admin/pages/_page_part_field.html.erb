<div class="page-part" id="<%= part.to_param %>">
  <%= label_tag "page_parts_attributes_#{part_index}_body", part.title, :class => 'js-hide' %>
  <%= hidden_field_tag "page[parts_attributes][#{part_index}][title]", part.title if new_part %>
  <%= text_area_tag "page[parts_attributes][#{part_index}][body]", part.body, :rows => 20, :cols => 120, :class => 'editable-content' %>
  <%= hidden_field_tag "page[parts_attributes][#{part_index}][position]", part_index %>
</div>
